{
  "name": "@server/auth",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "apps/auth/dist",
          "format": [
            "cjs"
          ],
          "bundle": true,
          "main": "apps/auth/src/main.ts",
          "tsConfig": "apps/auth/tsconfig.app.json",
          "assets": [
            {
              "input": "libs/emails/src/lib/templates",
              "glob": "**/*",
              "output": "templates"
            },
            {
              "input": "apps/auth/assets",
              "glob": "**/*",
              "output": "."
            }
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/auth/dist/package.json",
          "{workspaceRoot}/apps/auth/dist/pnpm-lock.yaml"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/auth/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@server/auth:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@server/auth:build:development"
          },
          "production": {
            "buildTarget": "@server/auth:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "@server/cloud": "workspace:*",
    "@server/emails": "workspace:*",
    "@server/middlewares": "workspace:*",
    "@server/protect": "workspace:*",
    "@server/security": "workspace:*",
    "@server/types": "workspace:*",
    "@server/upload": "workspace:*",
    "@server/utils": "workspace:*",
    "@server/validations": "workspace:*",
    "@simplewebauthn/server": "^13.2.2",
    "@upstash/redis": "^1.35.7",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.1",
    "cloudinary": "^2.8.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.22.1",
    "express-http-proxy": "^2.1.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-useragent": "1.0.15",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "ipinfo-express": "^2.2.0",
    "jsonwebtoken": "^9.0.3",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.20.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express-session": "^1.18.2",
    "@types/express-useragent": "^1.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.15",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-twitter": "^1.0.40",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10"
  }
}
